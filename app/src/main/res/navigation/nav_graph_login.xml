<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_login"
    app:startDestination="@id/fragment_login">

    <fragment
        android:id="@+id/fragment_login"
        android:name="com.ssafy.smartstore_jetpack.presentation.views.signin.login.LoginFragment"
        android:label="Login"
        tools:layout="@layout/fragment_login">

        <action
            android:id="@+id/action_login_to_join"
            app:destination="@id/fragment_join" />

    </fragment>

    <fragment
        android:id="@+id/fragment_join"
        android:name="com.ssafy.smartstore_jetpack.presentation.views.signin.join.JoinFragment"
        android:label="Join"
        tools:layout="@layout/fragment_join">


    </fragment>

</navigation>
